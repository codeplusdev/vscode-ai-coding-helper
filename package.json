{
  "name": "vs-ai-coding-helper-by-codeplusdev",
  "displayName": "VS AI Coding Helper by CodePlusDev",
  "description": "A Coding helper VS Code plugin that create, analyze or update your code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.findGptComments"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "GPT-3 Integration",
      "properties": {
        "gpt3integration.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API Key for accessing GPT-3 services"
        },
        "gpt3integration.gptModel": {
          "type": "string",
          "default": "gpt-3.5-turbo",
          "description": "OpenAI API Key for accessing GPT-3 services"
        }
      }
    },
    "commands": [
      {
        "command": "extension.findGptComments",
        "title": "Insert GPT Code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "ts-loader": "^9.5.1",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}